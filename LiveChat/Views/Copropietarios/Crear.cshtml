@model CrearModel
<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">Crear Cotitular</h1>
    </div>
    <!-- /.col-lg-12 -->
</div>
<!-- /.row -->
<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-default">
            <div class="panel-heading">
                Agrega a un nuevo Cotitular
            </div>
            <div class="panel-body">
                <div class="row">
                    <div class="col-lg-12">
                        <form method="post" asp-controller="Copropietarios" asp-action="Crear">
                            <div class="form-group">
                                <label>Nombres</label>
                                <input asp-for="Nombres" class="form-control" placeholder="Escriba el nombre">
                            </div>
                            <div class="form-group">
                                <label>Apellidos</label>
                                <input asp-for="Apellidos" class="form-control" placeholder="Escriba el apellido">
                            </div>
                            <div class="form-group">
                                <label>E-mail</label>
                                <input asp-for="Email" class="form-control" placeholder="Escriba el Email">
                            </div>
                            <div class="form-group">
                                <label>Telefono</label>
                                <input asp-for="Phone" class="form-control" placeholder="Escriba el Telefono">
                            </div>
                            <input type="submit" class="btn btn-success" value="Agregar Cotitular"/>
                        </form>
                    </div>
                    <!-- /.col-lg-6 (nested) -->
                </div>
                <!-- /.row (nested) -->
            </div>
            <!-- /.panel-body -->
        </div>
        <!-- /.panel -->
    </div>
    <!-- /.col-lg-12 -->
</div>